<%= form_with(url: "posts_path", method: "post") do %>
    
  <div class="form-group">
    <%= text_field_tag(:title , nil , placeholder:'title' ,class:"form-control" )%>
    </div>
    <div class="form-group">
    <%= text_area_tag(:title , nil , placeholder:'title' ,class:"form-control" )%>
    </div>
    <%= submit_tag("create" ,class:"btn btn-info btn-block") %>
  <% end %>