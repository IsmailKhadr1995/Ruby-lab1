

<% @posts.each do |post| %>
<div class="card md-12 mb-3" >
   <div class="card-body">
   <h1 class="card-title"><%=post.user.name%></h1>
    <h5 class="card-title"><%=post.title%></h5>
    <p class="card-text"><%=post.content%></p>
   
    <a href="<%=edit_post_path(post)	%>" class="btn btn-primary">Update</a>
    <%=link_to 'Delete' ,post , method:  :DELETE , class: "btn btn-danger"%>
    <a href="<%=post_path(post)	%>" class="btn btn-primary">ShowComment</a>
    </div>
</div>
<%end%> 